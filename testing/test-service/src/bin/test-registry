#!/usr/bin/env node

const express = require('express');
const app = express();

app.get('/', (req, res) => {

    res.json(
        { "data": {
          "lorem-fitsum": {
            "data-url" : "http://localhost:3000/api/rpde/session-series",
            "available" : true,
            "uses-paging-spec": true
            }
          }
        }
    );

});

app.listen(3001, () => { console.log("started http://locahost:3001"); } );